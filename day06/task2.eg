# Advent of Code (AoC) - day 6, task 2

import "prelude.eg"
import "os.ego"
import "regex.ego"

using System
using OS
using List

def find = [ N CC -> 
    if CC |> take 14 |> unique |> length |> ((==) 14)
    then N + 14 else find (N + 1) (tail CC) ]

def main =
    read_line stdin |> unpack |> find 0
